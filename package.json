{
  "name": "@nestjs/boiler",
  "version": "1.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "NODE_ENV=production",
    "start": "nest start",
    "prestart:dev": "NODE_ENV=development",
    "start:dev": "nest start --watch",
    "prestart:debug": "NODE_ENV=development",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "start:production": "npm run start:prod",
    "start:development": "npm run start:dev",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "npm rebuild bcrypt --build-from-source "
  },
  "dependencies": {
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "0.2.2",
    "@nestjs/core": "^6.10.14",
    "@nestjs/graphql": "6.5.4",
    "@nestjs/jwt": "6.1.1",
    "@nestjs/passport": "6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/typeorm": "6.3.0",
    "@types/bcrypt": "3.0.0",
    "bcrypt": "3.0.8",
    "class-transformer": "0.2.3",
    "class-validator": "0.11.0",
    "compression": "1.7.4",
    "express-rate-limit": "5.1.1",
    "graphql": "14.6.0",
    "graphql-tools": "4.0.6",
    "helmet": "3.21.2",
    "mysql": "2.18.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "6.11.7",
    "@types/express": "4.17.2",
    "@types/jest": "25.1.1",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "node-mocks-http": "1.8.1",
    "prettier": "^1.18.2",
    "rimraf": "3.0.2",
    "sqlite3": "4.1.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engine": {
    "node": "12.13"
  }
}
